{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "go: build sepaqx",
      "type": "shell",
      "command": "/usr/local/go/bin/go",
      "args": [
        "build",
        "-v",
        "-o",
        "${workspaceFolder}/.bin/sepaqx",
        "./"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$go"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
